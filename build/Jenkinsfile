node {
   echo '开始构建......'
   stage('Git') {
       checkout()
       sh 'nrm use taobao; nrm current; nvm current;'
   }
   stage('Test') {
       echo '开始运行测试用例 ....'
   }
   stage('Build') {
       echo '开始执行构建 ....'
   }
   stage('NPM') {
       sh 'nrm use npm; nrm current; nvm current;'
       echo '开始发布产品 ....'
   }
}